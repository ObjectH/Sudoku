                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:padding="0dp"
                android:text="H"
                android:textSize="34sp"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/button_A"
                app:layout_constraintEnd_toStartOf="@id/button_I"
