            <Button
                android:id="@+id/button_A"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:padding="0dp"
                android:text="A"
                android:textSize="34sp"
                android:visibility="gone"
